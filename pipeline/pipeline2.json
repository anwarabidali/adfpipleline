{
	"name": "pipeline2",
	"properties": {
		"activities": [
			{
				"name": "CopyfromBLOBtoBLOB",
				"type": "Copy",
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"source": {
						"type": "BlobSource",
						"recursive": true
					},
					"sink": {
						"type": "BlobSink"
					},
					"enableStaging": false,
					"dataIntegrationUnits": 0
				},
				"inputs": [
					{
						"referenceName": "AzureBlobSrcDS",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "AzureBlobStgDS",
						"type": "DatasetReference",
						"parameters": {
							"filename": "@pipeline().parameters.file1"
						}
					}
				]
			}
		],
		"parameters": {
			"file1": {
				"type": "String",
				"defaultValue": "@pipeline().parameters.filename"
			}
		}
	}
}